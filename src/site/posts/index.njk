---
title: Updates and blog
subtitle: What's new at GeneticScores.org
layout: layouts/base.njk
---

{% render '@vf-intro', {"vf_intro_phase": "", "vf_intro_heading": title,
  "vf_intro_lede": "",
  "vf_intro_subheading": subtitle,
  "vf_intro_badge": "",
  "vf_intro_text": [
    ""
  ]
} %}


<section class="embl-grid embl-grid--has-centered-content">
  <div>
    <!-- empty -->
  </div>
  <div class="">
{%- for post in collections.posts | reverse %}
<article class="vf-summary vf-summary--news">
  <span class="vf-summary__date">{{ post.date | dateDisplay }}</span>
  <img class="vf-summary__image" src="{{ post.data.meta.image | url }}" alt="">
  <h3 class="vf-summary__title">
    <a href="{{ post.url }}" class="vf-summary__link">{{ post.data.title }}</a>
  </h3>
  <p class="vf-summary__text">
    {{ post.data.subtitle }}
  </p>
</article>
{%- endfor %}
  </div>
  <div></div>
</section>
